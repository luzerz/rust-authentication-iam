  "query": "\n            SELECT p.id, p.name, p.effect, p.conditions_json, p.priority, p.conflict_resolution\n            FROM abac_policies p\n            INNER JOIN user_abac_policies uap ON uap.policy_id = p.id\n            WHERE uap.user_id = $1\n            ORDER BY p.priority DESC\n            ",
